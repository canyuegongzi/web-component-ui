{
  "name": "root",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "build:tsc": "cross-env NODE_ENV=production tsc -p tsconfig.build.json",
    "dev:example": "cd example && npm run webpack:dev",
    "build:ex": "cd example && npm run build",
    "release:ex": "npm run build:ex && node ./scripts/releaseExample.js",
    "release:next": "npm run cp-md && npm run build && lerna publish --force-publish --dist-tag next",
    "release:last": "npm run cp-md && npm run build && lerna publish --force-publish",
    "rm": "rimraf packages/**/dist",
    "clean": "rimraf node_modules",
    "cp-md": "node  ./scripts/cp-md",
    "build": "npm-run-all rm build:umd build:cjs build:es build:dts build:dts",
    "build:cjs": "node ./scripts/build.cjs",
    "build:es": "node ./scripts/build.es",
    "build:umd": "node ./scripts/build.umd && node ./scripts/build.umd --compress",
    "build:dts": "rimraf packages/*/types && node ./scripts/build.dts",
    "release": "npm run test && standard-version && npm run build && npm pub && git push",
    "release-no-test": "standard-version && npm run build && npm pub && git push",
    "dev": "rollup -c  --watch --environment NODE_ENV:development",
    "test": "jest",
    "test:q": "jest --collectCoverage false",
    "size": "node ./scripts/calcSize",
    "test-no-cache": "jest --no-cache",
    "removeAll": "rimraf ./node_modules",
    "commit": "git-cz",
    "version": "standard-version"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 10",
    "npm": ">= 5"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/jest": "^23.3.14",
    "@types/node": "^10.17.56",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-typescript": "^2.0.1",
    "@types/lodash": "^4.14.149",
    "brotli": "^1.3.2",
    "chalk": "^2.4.2",
    "gh-pages": "^2.0.1",
    "minimist": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-typescript2": "^0.25.3",
    "shelljs": "^0.8.3",
    "standard-version": "^4.4.0",
    "terser": "^4.6.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.8.3",
    "zlib": "^1.0.5",
    "lerna": "^4.0.0",
    "cross-env": "^7.0.3",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^23.6.0",
    "lodash": "^4.17.21",
    "nodemon": "^1.18.9",
    "parse-pkg-name": "^2.0.0",
    "prettier": "^1.15.3",
    "qs": "^6.10.1",
    "rollup": "^2.47.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-visualizer": "^5.5.0",
    "supertest": "^3.4.1",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1"
  }
}
